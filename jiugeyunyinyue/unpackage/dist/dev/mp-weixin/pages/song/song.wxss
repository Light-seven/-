.mohu {
  position: relative;
  z-index: 1;
  width: 100%;
  height: 100vh;
  background-size: contain;
}
.mohu:after {
  content: '';
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: inherit;
  -webkit-filter: blur(16rpx);
          filter: blur(16rpx);
  z-index: 2;
}
.songContainer {
  z-index: 22;
  background-size: contain;
}
.songContainer .songHeader {
  opacity: 1;
  padding-top: 40rpx;
  text-align: center;
  color: white;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.songContainer .songHeader .songHeaderCenter {
  z-index: 30;
  width: 500rpx;
  display: flex;
  flex-direction: column;
  font-size: 34rpx;
}
.songContainer .songHeader .songHeaderCenter .singer {
  z-index: 30;
  font-size: 28rpx;
  color: rgba(255, 255, 255, 0.7);
}
.songContainer .needle {
  position: relative;
  width: 192rpx;
  height: 274rpx;
  top: 20rpx;
  left: 335rpx;
  z-index: 99;
  -webkit-transform: rotateZ(-30deg);
          transform: rotateZ(-30deg);
  transition: -webkit-transform 1s;
  transition: transform 1s;
  transition: transform 1s, -webkit-transform 1s;
  -webkit-transform-origin: 40rpx 0;
          transform-origin: 40rpx 0;
}
.songContainer .songDis {
  z-index: 24;
  position: relative;
  width: 598rpx;
  height: 598rpx;
  left: 76rpx;
  top: -140rpx;
  -webkit-animation: move 4s linear infinite;
          animation: move 4s linear infinite;
  -webkit-animation-play-state: paused;
          animation-play-state: paused;
}
.songContainer .songDis .dis {
  position: relative;
  height: 100%;
  width: 100%;
  z-index: 90;
}
.songContainer .songDis .bigPic {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 370rpx;
  height: 370rpx;
  z-index: 89;
}
.songContainer .songContainerBtn {
  margin-top: 40rpx;
  display: flex;
  justify-content: space-around;
}
.songContainer .barContainer {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 35rpx;
}
.songContainer .barContainer text {
  z-index: 22;
  color: white;
  font-size: 22rpx;
}
.songContainer .barContainer .wrap {
  width: 450rpx;
  height: 4rpx;
  background: rgba(255, 255, 255, 0.5);
  margin: 0 20rpx;
  z-index: 22;
}
.songContainer .barContainer .wrap .inner {
  position: relative;
  width: 0%;
  height: 100%;
  background: white;
  z-index: 22;
}
.songContainer .barContainer .wrap .point {
  position: absolute;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: white;
  z-index: 23;
  top: -4rpx;
  right: -6rpx;
}
.songContainer .footerBtn {
  margin-top: 40rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.songContainer .footerBtn .fbtn {
  z-index: 22;
}
@-webkit-keyframes move {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes move {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.isplay .needle {
  -webkit-transform: rotate(0deg);
          transform: rotate(0deg);
}
.isplay .songDis {
  -webkit-animation-play-state: running;
          animation-play-state: running;
}

