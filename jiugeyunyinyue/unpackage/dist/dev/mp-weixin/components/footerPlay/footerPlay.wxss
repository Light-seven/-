::-webkit-scrollbar {
  display: none;
  width: 0 !important;
  height: 0 !important;
  -webkit-appearance: none;
  background: transparent;
  color: transparent;
}
.box {
  width: 100%;
  height: 110rpx;
  background-color: white;
}
.active {
  color: red;
}
.pop {
  margin: 0 30rpx 0 30rpx;
  /* margin-left: 30rpx;
   margin-right: 30rpx; */
  height: 50vh;
  background-color: white;
  border-radius: 20rpx;
  padding: 40rpx;
}
.pop .title {
  margin-bottom: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: black;
}
.pop .scroll-Y {
  height: 55vh;
}
.pop .list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 40rpx 0;
  height: 42rpx;
  width: 610rpx;
}
.pop .list .listLeft {
  width: 510rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
}
.pop .list .songName {
  font-size: 32rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.pop .list .singer {
  font-size: 26rpx;
  color: lightgray;
  overflow: hidden;
  white-space: nowrap;
}
.playBar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  /* position: sticky;
bottom: 0; */
  background-color: white;
  z-index: 99;
}
.playBar .playBarL {
  width: 550rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.playBar .playBarL image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-left: 30rpx;
  margin-right: 15rpx;
}
.playBar .playBarL .text {
  width: 400rpx;
  display: flex;
  align-items: center;
  height: 100rpx;
  line-height: 100rpx;
}
.playBar .playBarL .songName {
  overflow: hidden;
  white-space: nowrap;
  display: inline-block;
  text-overflow: ellipsis;
}
.playBar .playBarL .singer {
  margin-left: 10rpx;
  padding-top: 2rpx;
  font-size: 30rpx;
  color: darkgray;
  white-space: nowrap;
  display: inline-block;
  text-overflow: ellipsis;
}
.playBar .playBarR {
  height: 100%;
  z-index: 22;
  display: flex;
  align-items: center;
}
.playBar .playBarR .btn {
  margin-left: 20rpx;
}

